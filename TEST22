// Excample7_4.java
import java.util.Arrays;

public class Excample7_4 {
	public static void main(String args[]) {
		int []a= {11,22,33,44,55,66},b,c,d;
		b=Arrays.copyOf(a,8);
		System.out.println("数组a各个元素的值为：");
		System.out.println(Arrays.toString(a));
		System.out.println("数组b各个元素的值为：");
		System.out.println(Arrays.toString(b));
		c=Arrays.copyOfRange(a,3,5);
		System.out.println("数组c各个元素的值为：");
		System.out.println(Arrays.toString(c));
		d=Arrays.copyOfRange(a,1,7);
		System.out.println("数组d各个元素的值为：");
		System.out.println(Arrays.toString(d));
	}

}
